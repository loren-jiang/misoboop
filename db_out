CREATE TABLE public.recipe_recipe (
    id integer NOT NULL,
    created_at timestamp with time zone,
    modified_at timestamp with time zone,
    name character varying(500) NOT NULL,
    short_description text NOT NULL,
    description text NOT NULL,
    prep_time smallint NOT NULL,
    cook_time smallint NOT NULL,
    servings smallint NOT NULL,
    max_servings smallint NOT NULL,
    is_published boolean NOT NULL,
    slug character varying(100) NOT NULL,
    likes smallint NOT NULL,
    author_id integer,
    image_id integer,
    series_id integer,
    placeholder_url character varying(300) NOT NULL,
    CONSTRAINT recipe_recipe_cook_time_check CHECK ((cook_time >= 0)),
    CONSTRAINT recipe_recipe_likes_check CHECK ((likes >= 0)),
    CONSTRAINT recipe_recipe_max_servings_check CHECK ((max_servings >= 0)),
    CONSTRAINT recipe_recipe_prep_time_check CHECK ((prep_time >= 0)),
    CONSTRAINT recipe_recipe_servings_check CHECK ((servings >= 0))
);


